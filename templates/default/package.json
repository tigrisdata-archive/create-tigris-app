{
  "name": "default",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean && npm install",
    "build": "npx tsc",
    "postbuild": "npm run setup",
    "setup": "npx ts-node setup.ts",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "predev": "npm run setup",
    "dev": "npx nodemon"
  },
  "dependencies": {
    "@tigrisdata/core": "1.0.0-dev.1",
    "dotenv": "16.0.3"
  },
  "devDependencies": {
    "@types/node": "18.11.11",
    "eslint": "8.29.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts",
    "ignore": [
      "**/test/**",
      "**/docs/**"
    ],
    "delay": 2500,
    "exec": "npx ts-node ./src/index.ts"
  }
}
