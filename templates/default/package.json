{
  "name": "default",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean && npm install",
    "build": "npx tsc",
    "postbuild": "npm run setup",
    "setup": "npx ts-node setup.ts",
    "prestart": "npm run build",
