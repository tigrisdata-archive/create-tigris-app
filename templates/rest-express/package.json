  },
  "dependencies": {
    "@tigrisdata/core": "1.0.0-beta.21",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/node": "18.11.11",
    "eslint": "8.29.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  }
}
